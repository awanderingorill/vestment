    margin: 30px auto;
    font-size: 14px;
}

.customer_address_table input {
  font-family: 'futura-med', Helvetica, sans-serif !important;
  width: 100%;
  padding: 10px;
  box-sizing: border-box;
  margin-bottom: 10px;
  font-size: 14px;
}

.customer_address_table input[type="checkbox"] {
    display: inline-block;
    width: 20px;
    text-align: left;
}

.customer_address_table .label {
  text-align: left; 
}

.action_bottom .add-address-btn {
    width: 135px;
    height: 36px;
    border-radius: 0;
    outline: 0;
    background: #000;
    font-family: 'futura-med', Helvetica, sans-serif;
    text-align: center;
    font-size: 14px;
    padding-left: 10px;
    cursor: pointer;
  	color: #fff;
  	text-transform: uppercase;
}

.action_bottom .note a {
	margin-bottom: 30px;
  	text-decoration: underline;
}

.add-address-btn:hover, {
	opacity: 0.7;
}

.address_actions a {
	color: #aaa;
  	margin: 10px 5px;
  	display: inline-block;
}

.address_actions a:hover {
  color: #000;
}


.customer_address_table select {
  font-family: 'futura-med', Helvetica, sans-serif !important;
    margin: 10px 0;
    border: 1px solid #000;
    border-radius: 0;
    -webkit-appearance: none;
    padding: 10px;
  width: 100%;
  background: #fff;
}

.customer_address_table .select-drop {
  position: relative;
  text-align: right;
}
.customer_address_table .select-drop {
  text-align: left;
}


.customer_address_table .select-drop:after {
	content: "";
  	display: block;
  	position: absolute;
  	right: 12px;
  	top: 25px;
  	border-top: 5px solid #000;
  	border-left: 5px solid transparent;
    border-right: 5px solid transparent;
}

#add_address {
  margin-top: -50px;
}


a#view_address, .action_add a {
  color: #000;
  border: 1px solid #000;
  padding:15px 10px;
  box-sizing: border-box;
  text-decoration:none;
  text-transform: uppercase;
  display: inline-block;
  margin: 40px 0 80px;
}

.return a {
  color: #fff;
  background: #000;
  padding:15px 10px;
  box-sizing: border-box;
  text-decoration:none;
  text-transform:uppercase;
  margin-top: 80px;
  display: inline-block;
}

.shop-button.mobile {
  display:none;
}

.product-page-top .bag-container {
    position: fixed;
    top:12px;
    left: 15px;
  }	

a#view_address:hover, .return a:hover { opacity: .6;}


